{
  "name": "parcel-github-pages-boilerplate",
  "version": "1.0.0",
  "description": "A Parcel boilerplate to deploy on Github Pages using Github Actions.",
  "scripts": {
    "start": "rm -rf .cache dist && parcel index.njk",
    "build": "rm -rf .cache dist && parcel build index.njk --public-url ./",
    "lint": "eslint \".*.js\" \"src/**/*.js\"",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "eslint": "^7.25.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-standard": "^5.0.0",
    "marked": "^2.0.3",
    "nunjucks": "^3.2.3",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-nunjucks": "^2.2.2",
    "sass": "^1.32.12"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "bootstrap": "^4.6.0",
    "core-js": "^3.11.2",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "regenerator-runtime": "^0.13.7"
  }
}
